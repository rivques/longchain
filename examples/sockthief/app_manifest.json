{
    "display_information": {
        "name": "Sock Thief Quest",
        "description": "Ping me to go on a quest in the Bag universe!",
        "background_color": "#00248f"
    },
    "features": {
        "bot_user": {
            "display_name": "Sock Thief Quest",
            "always_online": true
        },
        "slash_commands": [
            {
                "command": "/st-reset-user",
                "description": "delete a user's data",
                "usage_hint": "<@user>",
                "should_escape": true
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "app_mentions:read",
                "channels:history",
                "chat:write",
                "chat:write.customize",
                "commands",
                "users:read",
                "reactions:write"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "app_mention",
                "message.channels"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}